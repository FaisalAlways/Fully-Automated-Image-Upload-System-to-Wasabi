generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_POSTGRES")
}

// ==========================
//          Image Upload
// ==========================
model Image {
  id        String   @id @default(uuid())
  key       String
  bucket    String
  folder    String?
  size      Int
  filetype  String?
  createdAt DateTime @default(now())
}
